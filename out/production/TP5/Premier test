public class Main{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        double[] tableau = new double[]{-3,1,5,7,85,95,65,2};
        for (int i = 1; i < tableau.length; i++) {
            double val = tableau[i];
            for (int j = i+1; j < tableau.length; j++){
                if (val<tableau[j] == false) {
                    val = tableau[j];
                    tableau[j]=tableau[i];
                    tableau[i]=val;

                }
            }
        }
    }
}